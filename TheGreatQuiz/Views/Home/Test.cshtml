@{
    Layout = null;
}


<!DOCTYPE html>
<html ng-app="myApp" lang="en">
<head>
    <meta charset="UTF-8">
    <title>Document</title>
    <script src="~/Scripts/angular.min.js"></script>
    <link rel="stylesheet" href="~/Content/Test.css" />
    <link rel="stylesheet" href="~/Content/unsemantic-grid-responsive-tablet.css" />
</head>
<body>
    <header class="mainHeader"></header>
    <div class="grid-container grid-90 prefix-5">


        <section class="testWindow grid-40 grid-parent">
            
            <nav class="testHeader">
              <div class="grid-60"><br /></div>                        
              <button ng-click="CurrentTests()" class="grid-20 testButton">Aktuella</button>                                         
              <button class="grid-20 testButton">Färdiga</button>                  
            </nav>
            <artile class="grid-100">
                <ul>
                    <li>{{Tests}}</li>
                    <li class="Tests">
                        <a class="grid-100 Link" href="">Matematik A</a>            
                    </li>
                    <li class="Tests">
                        <a class="grid-100 Link" href="">blablabla</a>
                    </li>
                </ul> 
                <form>
                    <label>Till:</label> <br />
                    <input ng-model="name" type="text" /> <br />
                    <label>Meddelande:</label> <br />
                    <textarea ng-model="message"></textarea> <br />
                </form>
                <h2>{{ name }}</h2>
                <p>{{ message }}</p>
                <script src="~/Content/whatTests.js"></script>              
            </artile>
        </section>

    </div>

</body>
</html>


